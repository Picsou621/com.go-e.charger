{
  "name": {
    "en": "go-e Charger 1st gen CC1"
  },
  "class": "other",
  "capabilities": [
    "onoff_charging_allowed",
    "meter_power",
    "measure_power",
    "measure_current",
    "measure_voltage",
    "measure_temperature",
    "current_limit",
    "current_max",
    "status",
    "is_charging",
    "is_connected",
    "is_device_error",
    "energy_total"
  ],
  "capabilitiesOptions": {
    "measure_temperature": {
      "type": "number",
      "title": {
        "en": "Temperature",
        "nl": "Temperatuur"
      },
      "getable": true,
      "setable": false,
      "units": {
        "en": "C"
      },
      "min": -50,
      "max": 50,
      "insights": true    
    },
    "status": {
      "type": "enum",
      "title": {
        "en": "Device Status",
        "nl":"Status van het apparaat"
      },
      "values": [
        {
          "id": "station_idle",
          "title": {
            "en": "No car connected",
            "nl": "Geen auto verbonden"
          }
        },
        {
          "id": "car_charging",
          "title": {
            "en": "Charging",
            "nl": "Auto opladen"
          }
        },
        {
          "id": "car_waiting",
          "title": {
            "en": "Waiting to charge",
            "nl": "Klaarmaken voor laden"
          }
        },
        {
          "id": "car_finished",
          "title": {
            "en": "Charging stopped",
            "nl": "Klaar met laden"
          }
        }
      ],
      "setable": false,
      "getable": true,
      "insights": false
    }
  },
  "platforms": [
    "local"
  ],
  "connectivity": [
    "lan"
  ],
  "images": {
    "small": "{{driverAssetsPath}}/images/small.png",
    "large": "{{driverAssetsPath}}/images/large.png",
    "xlarge": "{{driverAssetsPath}}/images/xlarge.png"
  },
  "discovery": "mdns_http_go-e",
  "pair": [
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ]
}